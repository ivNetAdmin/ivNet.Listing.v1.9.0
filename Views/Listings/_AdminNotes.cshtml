<div class="form-horizontal col-md-12 owner-detail">
    <div class="col-md-12">
        <h4>Admin Notes </h4>
        <hr />
    </div>
    <div class="form-group">
        <label class="control-label col-md-2" for="OwnerName">Owner</label>
        <div class="col-md-10">
            <span>{{ownerName}}. {{email}}</span>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2" for="Notes">Notes</label>
        <div class="col-md-10">
            <textarea name="Notes" id="Notes" rows="3" cols="80" ng-model="notes"></textarea>
        </div>
    </div>
</div>

